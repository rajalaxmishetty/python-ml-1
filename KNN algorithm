      "source": [
        "x= df.iloc[:,0:8]\n",
        "y = df.iloc[:,8]\n",
        "\n",
        "# splitting our data into Training & Testing data sets in the ratio of 8:2 for both dependent and independent\n",
        "x_train,x_test, y_train,y_test = train_test_split(x,y, test_size= 0.2, random_state= 123)"
      ],
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
